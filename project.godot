[android]

modules="org/godotengine/godot/GodotAdMob"

[application]

config/name="Pixel Zone"
config/description="A short game made with Godot Engine"
run/main_scene="res://scenes/intro_low_size.tscn"
boot_splash/image="res://boot/logo/logo.png"
boot_splash/use_filter=false
config/icon="res://icon.png"
config/macos_native_icon="res://icon.icns"
config/windows_native_icon="res://icon.ico"

[autoload]

globals="*res://globals/globals.gd"
game_state="*res://globals/game_state.gd"
gamestate="*res://globals/gamestate.gd"
background_load="*res://scenes/background_load.tscn"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
AdvancedBackgroundLoader="*res://addons/Advanced Background Loader/background_load.gd"
admob="*res://admob-lib/admob.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/dpi/allow_hidpi=true
window/vsync/vsync_via_compositor=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "Advanced Background Loader", "adb_helper", "silent_wolf" )

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

